{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build", "^prisma:generate"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "outputs": []
    },
    "prisma:generate": {
      "cache": false
    },
    "//#dev:sst": {
      "cache": false
    },
    "dev:next": {
      "cache": false
    },
    "dev": {
      "dependsOn": ["prisma:generate", "^//#dev:sst", "dev:next"],
      "cache": false
    }
  }
}
