{
  "name": "evergreendocs",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*",
    "terraform/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@vitest/ui": "^0.28.4",
    "prettier": "latest",
    "turbo": "latest",
    "vitest": "^0.28.4"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "yarn@3.4.1"
}
